.mainHeader
  wrapped(90%)
  position absolute
  top 2rem
  right 0
  left 0
  display flex
  align-items center
  justify-content space-between
  z-index 10
  
  +below($mobile)
    justify-content center
    position static
    margin-top 2rem
  
.mainHeader-logo
  width 260px
  text-decoration none
  
  & img
    width 100%
    display block
    