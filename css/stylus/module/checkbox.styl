.checkboxRadio 
  margin-top 1rem
  
.checkboxRadio-1
  margin-right 2rem
  background-color #00285A
  border-radius 50%
  border 2px solid white

.checkboxRadio--active 
  box-shadow 0 2px 4px 0 rgba(0,0,0,0.50)

.checkboxRadio-2
  background-color $green
  border-radius 50%
  border 2px solid white